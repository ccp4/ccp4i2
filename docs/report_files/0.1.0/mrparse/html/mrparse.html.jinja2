<body>
    <div class="toggle-switch-container">
        <div class="toggle-switch">
            <input type="checkbox" id="dark-mode-switch">
            <label for="dark-mode-switch" class="switch-label">
                <span class="switch-inner"></span>
                <span class="switch-slider"></span>
            </label>
        </div>
    </div>
    <div id="app">
        <h3 id="hklinfo-title" class="collapsible1">HKL Information</h3>
        <div id="div1" class="content">
            <hklinfo-table></hklinfo-table>
        </div>

        <h3 id="seqinfo-title" class="collapsible2">Sequence based predictions</h3>
        <div id="div2" class="content">
            <hklinfo-feature-viewer></hklinfo-feature-viewer>
        </div>

        <h3 id="pdb-title" class="collapsible3">Experimental structures from the PDB</h3>
        <div id="div3" class="content">
            <pdb-table></pdb-table>
            <pdb-feature-viewer></pdb-feature-viewer>
        </div>

        <h3 id="af-title" class="collapsible4">Structure predictions from the EBI AlphaFold database</h3>
        <div id="div4" class="content">
            <af-table></af-table>
            <af-feature-viewer></af-feature-viewer>
        </div>

        <h3 id="bfvd-title" class="collapsible5">Structure predictions from the Big Fantastic Virus Database</h3>
        <div id="div5" class="content">
            <bfvd-table></bfvd-table>
            <bfvd-feature-viewer></bfvd-feature-viewer>
        </div>

        <h3 id="esm-title" class="collapsible6">Structure predictions from the ESMfold Atlas database</h3>
        <div id="div6" class="content">
            <esm-table></esm-table>
            <esm-feature-viewer></esm-feature-viewer>
        </div>
    </div>
    <script type="text/javascript" src="{{ mrparse_html_dir }}/mrparse_vue.js"></script>
</body>
