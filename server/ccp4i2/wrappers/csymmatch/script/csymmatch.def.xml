<?xml version='1.0' encoding='ASCII'?>
<ccp4i2>
  <ccp4i2_header>
    <function>DEF</function>
    <pluginName>csymmatch</pluginName>
  </ccp4i2_header>
  <ccp4i2_body id="csymmatch">
    <container id="inputData">
      <content id="XYZIN_QUERY">
        <className>CPdbDataFile</className>
        <qualifiers>
          <mustExist>True</mustExist>
          <allowUndefined>False</allowUndefined>
          <fromPreviousJob>False</fromPreviousJob>
	  <ifAtomSelection>True</ifAtomSelection>
        </qualifiers>
      </content>
      <content id="XYZIN_TARGET">
        <className>CPdbDataFile</className>
        <qualifiers>
          <mustExist>True</mustExist>
          <allowUndefined>False</allowUndefined>
          <fromPreviousJob>False</fromPreviousJob>
	  <ifAtomSelection>True</ifAtomSelection>
        </qualifiers>
      </content>
    </container>
    <container id="outputData">
      <content id="XYZOUT">
        <className>CPdbDataFile</className>
        <qualifiers>
          <allowUndefined>True</allowUndefined>
          <mustExist>False</mustExist>
        </qualifiers>
      </content>
    </container>
    <container id="controlParameters">
      <content id="ORIGIN_HAND">
        <className>CBoolean</className>
        <qualifiers>
          <default>True</default>
        </qualifiers>
      </content>
      <content id="CONNECTIVITY_RADIUS">
          <className>CFloat</className>
          <qualifiers>
          </qualifiers>
      </content>
    </container>
  </ccp4i2_body>
</ccp4i2>
