<?xml version='1.0' encoding='ASCII'?>
<ccp4:ccp4i2 xmlns:ccp4="http://www.ccp4.ac.uk/ccp4ns">
    <ccp4i2_header>
        <function>DEF</function>
        <pluginName>coot_find_ligand</pluginName>
    </ccp4i2_header>
    <ccp4i2_body id="coot_find_ligand">
        <container id="inputData">
            <content id="XYZIN">
                <className>CPdbDataFile</className>
                <qualifiers>
                    <allowUndefined>False</allowUndefined>
                    <fromPreviousJob>True</fromPreviousJob>
                </qualifiers>
            </content>
            <content id="FPHI">
                <className>CMapCoeffsDataFile</className>
                <qualifiers>
                    <allowUndefined>False</allowUndefined>
                    <fromPreviousJob>True</fromPreviousJob>
                    <requiredSubType>1</requiredSubType>
                </qualifiers>
            </content>
            <content id='COMPID'>
                <className>CString</className>
                <qualifiers>
                    <allowUndefined>False</allowUndefined>
                    <minLength>1</minLength>
                    <maxLength>5</maxLength>
                    <default>LIG</default>
                </qualifiers>
            </content>
            <content id="DICT">
                <className>CDictDataFile</className>
                <qualifiers>
                    <allowUndefined>False</allowUndefined>
                    <fromPreviousJob>True</fromPreviousJob>
                </qualifiers>
            </content>
        </container>
        <container id="controlParameters">
            <content id="THRESHOLD">
                <className>CFloat</className>
                <qualifiers>
                    <default>1.2</default>
                </qualifiers>
            </content>
            <content id="FLEXIBLE">
                <className>CBoolean</className>
                <qualifiers>
                    <default>True</default>
                </qualifiers>
            </content>
            <content id="CONFORMERS">
                <className>CInt</className>
                <qualifiers>
                    <default>50</default>
                    <min>1</min>
                </qualifiers>
            </content>
        </container>
        <container id="outputData">
            <content id="XYZOUT">
                <className>CPdbDataFile</className>
                <qualifiers>
                    <subType>1</subType>
                </qualifiers>
            </content>
        </container>
    </ccp4i2_body>
</ccp4:ccp4i2>