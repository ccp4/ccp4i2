<?xml version='1.0'?>
<ccp4i2>
    <ccp4i2_header>
        <function>DEF</function>
        <pluginName>cmapcoeff</pluginName>
    </ccp4i2_header>

    <ccp4i2_body>

        <container id="inputData">
            <content id="F_SIGF1">
                <className>CObsDataFile</className>
                <qualifiers>
                    <mustExist>True</mustExist>
                    <requiredContentFlag>1,2,3,4</requiredContentFlag>
                    <allowUndefined>False</allowUndefined>
                    <fromPreviousJob>True</fromPreviousJob>
                </qualifiers>
            </content>

            <content id="ABCD1">
                <className>CPhsDataFile</className>
                <qualifiers>
                    <requiredContentFlag>1,2</requiredContentFlag>
                    <allowUndefined>False</allowUndefined>
                    <fromPreviousJob>True</fromPreviousJob>
                    <sameCrystalAs>F_SIGF1</sameCrystalAs>
                </qualifiers>
            </content>

            <content id="F_SIGF2">
                <className>CObsDataFile</className>
                <qualifiers>
                    <mustExist>True</mustExist>
                    <requiredContentFlag>1,2,3,4</requiredContentFlag>
                    <allowUndefined>True</allowUndefined>
                    <sameCrystalAs>F_SIGF1</sameCrystalAs>
                    <fromPreviousJob>True</fromPreviousJob>
                </qualifiers>
            </content>

            <content id="ABCD2">
                <className>CPhsDataFile</className>
                <qualifiers>
                    <requiredContentFlag>1,2</requiredContentFlag>
                    <allowUndefined>True</allowUndefined>
                    <fromPreviousJob>True</fromPreviousJob>
                    <sameCrystalAs>F_SIGF1</sameCrystalAs>
                </qualifiers>
            </content>

            <content id="RESOLUTION">
                <className>CFloat</className>
            </content>

            <content id="INDEX_U">
                <className>CInt</className>
            </content>

            <content id="INDEX_V">
                <className>CInt</className>
            </content>

            <content id="INDEX_W">
                <className>CInt</className>
            </content>


        </container>



        <container id="outputData">

            <content id="FPHIOUT">
                <className>CMapCoeffsDataFile</className>
                <qualifiers>
                    <saveToDb>True</saveToDb>
                    <default>
                        <contentFlag>1</contentFlag>
                        <subType>1</subType>
                    </default>
                </qualifiers>
            </content>

            <content id="MAPOUT">
                <className>CMapDataFile</className>
                <qualifiers>
                    <default>
                        <subType>2</subType>
                        <contentFlag>1</contentFlag>
                    </default>
                    <allowUndefined>True</allowUndefined>
                </qualifiers>
            </content>

            <content id="XMLOUT">
                <className>CXmlDataFile</className>
                <qualifiers>
                    <saveToDb>False</saveToDb>
                </qualifiers>
            </content>

        </container>



        <container id="controlParameters">

            <content id="MAPTYPE">
                <className>CString</className>
                <qualifiers>
                  <onlyEnumerators>True</onlyEnumerators>
                  <menuText>Simple Fobs map,Anomalous difference map,Fobs - Fobs difference map</menuText>
                  <enumerators>fobs,anom,fobsfobs</enumerators>
                  <default>fobsfobs</default>
                </qualifiers>
            </content>

            <content id="MAP_OUTPUT">
                <className>CBoolean</className>
                <qualifiers>
                    <default>False</default>
                </qualifiers>
            </content>

            <content id="U_VALUE">
                <className>CFloat</className>
            </content>

            <content id="B_VALUE">
                <className>CFloat</className>
            </content>

            <content id="SCALE">
                <className>CBoolean</className>
                <qualifiers>
                    <default>True</default>
                </qualifiers>
            </content>

            <content id="F1_TO_F2">
                <className>CString</className>
                <qualifiers>
                    <onlyEnumerators>True</onlyEnumerators>
                    <menuText>Match first dataset to second dataset,Match second dataset to first dataset</menuText>
                    <enumerators>f1_to_f2,f2_to_f1</enumerators>
                    <default>f2_to_f1</default>
                </qualifiers>
            </content>

        </container>




    </ccp4i2_body>
</ccp4i2>
