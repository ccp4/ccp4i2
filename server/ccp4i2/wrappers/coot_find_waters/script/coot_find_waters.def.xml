<?xml version='1.0' encoding='ASCII'?>
<ccp4:ccp4i2 xmlns:ccp4="http://www.ccp4.ac.uk/ccp4ns">
    <ccp4i2_header>
        <function>DEF</function>
        <pluginName>coot_find_waters</pluginName>
    </ccp4i2_header>
    <ccp4i2_body id="coot_find_waters">
        <container id="inputData">
            <content id="XYZIN">
                <className>CPdbDataFile</className>
                <qualifiers>
                    <mustExist>True</mustExist>
                    <allowUndefined>False</allowUndefined>
                    <fromPreviousJob>True</fromPreviousJob>
                </qualifiers>
            </content>
            <content id="FPHIIN">
                <className>CMapCoeffsDataFile</className>
                <qualifiers>
                    <mustExist>True</mustExist>
                    <allowUndefined>False</allowUndefined>
                    <fromPreviousJob>True</fromPreviousJob>
                    <requiredSubType>1</requiredSubType>
                </qualifiers>
            </content>
        </container>
        <container id="controlParameters">
            <content id="THRESHOLD">
                <className>CFloat</className>
                <qualifiers>
                    <default>1.75</default>
                </qualifiers>
            </content>
            <content id="MINDIST">
                <className>CFloat</className>
                <qualifiers>
                    <default>2.4</default>
                </qualifiers>
            </content>
            <content id="MAXDIST">
                <className>CFloat</className>
                <qualifiers>
                    <default>3.4</default>
                </qualifiers>
            </content>
        </container>
        <container id="outputData">
            <content id="XYZOUT">
                <className>CPdbDataFile</className>
                <qualifiers>
                    <subType>1</subType>
                </qualifiers>
            </content>
        </container>
    </ccp4i2_body>
</ccp4:ccp4i2>
