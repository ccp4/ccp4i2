<?xml version='1.0' encoding='ASCII'?>
<ccp4i2>
  <ccp4i2_header>
    <function>DEF</function>
    <pluginName>phaser_analysis</pluginName>
  </ccp4i2_header>
  <ccp4i2_body id="phaser_analysis">
    <container id="inputData">
      <content id="HKLIN">
        <className>CMtzDataFile</className>
        <qualifiers>
          <mustExist>True</mustExist>
          <allowUndefined>False</allowUndefined>
        </qualifiers>
      </content>
      <content id="PXDNAME">
        <className>CString</className>
        <qualifiers>
          <allowUndefined>False</allowUndefined>
        </qualifiers>
      </content>
    </container>
    <container id="outputData"/>
    <container id="controlParameters">
      <content id="INFOCONTENTTHRESHOLD">
        <className>CFloat</className>
        <qualifiers>
          <guiLabel>None</guiLabel>
          <default>0.02</default>
          <toolTip>Information content threshold</toolTip>
          <min>0.0</min>
        </qualifiers>
      </content>
    </container>
  </ccp4i2_body>
</ccp4i2>
