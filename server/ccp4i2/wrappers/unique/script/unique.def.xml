<?xml version='1.0' encoding='ASCII'?>
<ccp4i2>
  <ccp4i2_header>
    <function>DEF</function>
    <pluginName>unique</pluginName>
  </ccp4i2_header>
  <ccp4i2_body id="unique">
    <container id="inputData">
      <content id="SPACEGROUPCELL">
        <className>CSpaceGroupCell</className>
      </content>
      <content id="RESOLUTION">
        <className>CFloat</className>
        <qualifiers>
          <min>0.0</min>
          <toolTip>Resolution in Angstrom</toolTip>
        </qualifiers>
      </content>
    </container>
    <container id="outputData">
      <content id="HKLOUT">
        <className>CMtzDataFile</className>
        <qualifiers/>
      </content>
      <content id="NO_REFLECTIONS">
        <className>CInt</className>
      </content>
    </container>
    <container id="controlParameters">
      <content id="TITLE">
        <className>CString</className>
      </content>
    </container>
  </ccp4i2_body>
</ccp4i2>
