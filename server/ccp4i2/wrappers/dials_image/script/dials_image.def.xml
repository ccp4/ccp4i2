<?xml version='1.0' encoding='ASCII'?>
<ccp4:ccp4i2 xmlns:ccp4="http://www.ccp4.ac.uk/ccp4ns">
    <ccp4i2_header>
        <function>DEF</function>
        <pluginName>dials_image</pluginName>
    </ccp4i2_header>
    <ccp4i2_body id="dials_image">
        <container id="inputData">
          <content id="JSON_IN">
            <className>CDialsJsonFile</className>
            <qualifiers>
              <mustExist>True</mustExist>
              <fromPreviousJob>True</fromPreviousJob>
              <allowUndefined>False</allowUndefined>
              <toolTip>DIALS .json file</toolTip>
            </qualifiers>
          </content>
          <content id="PICKLE_IN">
            <className>CDialsPickleFile</className>
            <qualifiers>
              <mustExist>True</mustExist>
              <fromPreviousJob>True</fromPreviousJob>
              <allowUndefined>True</allowUndefined>
              <toolTip>Dials Pickle File</toolTip>
            </qualifiers>
          </content>
        </container>
        <container id="outputData">
        </container>
    </ccp4i2_body>
</ccp4:ccp4i2>
