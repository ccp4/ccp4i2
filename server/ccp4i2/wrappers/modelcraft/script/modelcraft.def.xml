<?xml version='1.0' encoding='ASCII'?>
<ccp4i2>
    <ccp4i2_header>
        <function>DEF</function>
        <pluginName>modelcraft</pluginName>
    </ccp4i2_header>
    <ccp4i2_body id="modelcraft">
        <container id="inputData">
            <content id="F_SIGF">
                <className>CObsDataFile</className>
                <qualifiers>
                    <allowUndefined>False</allowUndefined>
                    <fromPreviousJob>True</fromPreviousJob>
                </qualifiers>
            </content>
            <content id="FREERFLAG">
                <className>CFreeRDataFile</className>
                <qualifiers>
                    <allowUndefined>False</allowUndefined>
                    <sameCrystalAs>F_SIGF</sameCrystalAs>
                    <fromPreviousJob>True</fromPreviousJob>
                </qualifiers>
            </content>
            <content id="PHASES">
                <className>CPhsDataFile</className>
                <qualifiers>
                    <allowUndefined>True</allowUndefined>
                    <sameCrystalAs>F_SIGF</sameCrystalAs>
                    <fromPreviousJob>True</fromPreviousJob>
                </qualifiers>
            </content>
            <content id="ASUIN">
                <className>CAsuDataFile</className>
                <qualifiers>
                    <allowUndefined>False</allowUndefined>
                    <fromPreviousJob>True</fromPreviousJob>
                </qualifiers>
            </content>
            <content id="XYZIN">
                <className>CPdbDataFile</className>
                <qualifiers>
                    <allowUndefined>True</allowUndefined>
                    <ifAtomSelection>True</ifAtomSelection>
                    <fromPreviousJob>True</fromPreviousJob>
                </qualifiers>
            </content>
        </container>
        <container id="controlParameters">
            <content id="USE_MODEL_PHASES">
                <className>CBoolean</className>
                <qualifiers>
                    <default>True</default>
                </qualifiers>
            </content>
            <content id="CYCLES">
                <className>CInt</className>
                <qualifiers>
                    <default>25</default>
                </qualifiers>
            </content>
            <content id="AUTO_STOP">
                <className>CBoolean</className>
                <qualifiers>
                    <default>True</default>
                </qualifiers>
            </content>
            <content id="STOP_CYCLES">
                <className>CInt</className>
                <qualifiers>
                    <default>4</default>
                </qualifiers>
            </content>
            <content id="BASIC">
                <className>CBoolean</className>
                <qualifiers>
                    <default>False</default>
                </qualifiers>
            </content>
            <content id="TWINNED">
                <className>CBoolean</className>
                <qualifiers>
                    <default>False</default>
                </qualifiers>
            </content>
            <content id="SHEETBEND">
                <className>CBoolean</className>
                <qualifiers>
                    <default>True</default>
                </qualifiers>
            </content>
            <content id="PRUNING">
                <className>CBoolean</className>
                <qualifiers>
                    <default>True</default>
                </qualifiers>
            </content>
            <content id="PARROT">
                <className>CBoolean</className>
                <qualifiers>
                    <default>True</default>
                </qualifiers>
            </content>
            <content id="DUMMY_ATOMS">
                <className>CBoolean</className>
                <qualifiers>
                    <default>True</default>
                </qualifiers>
            </content>
            <content id="WATERS">
                <className>CBoolean</className>
                <qualifiers>
                    <default>True</default>
                </qualifiers>
            </content>
            <content id="SIDE_CHAIN_FIXING">
                <className>CBoolean</className>
                <qualifiers>
                    <default>True</default>
                </qualifiers>
            </content>
            <content id="UNBIASED">
                <className>CBoolean</className>
                <qualifiers>
                    <default>False</default>
                </qualifiers>
            </content>
            <content id="SELENOMET">
                <className>CBoolean</className>
                <qualifiers>
                    <default>False</default>
                </qualifiers>
            </content>
        </container>
        <container id="outputData">
            <content id="XYZOUT">
                <className>CPdbDataFile</className>
                <qualifiers>
                    <default>
                        <annotation>ModelCraft model</annotation>
                        <subType>1</subType>
                        <contentFlag>2</contentFlag>
                    </default>
                </qualifiers>
            </content>
            <content id="FPHIOUT">
                <className>CMapCoeffsDataFile</className>
                <qualifiers>
                    <default>
                        <annotation>ModelCraft best map</annotation>
                        <subType>1</subType>
                        <contentFlag>1</contentFlag>
                    </default>
                </qualifiers>
            </content>
            <content id="DIFFPHIOUT">
                <className>CMapCoeffsDataFile</className>
                <qualifiers>
                    <default>
                        <annotation>ModelCraft difference map</annotation>
                        <subType>2</subType>
                        <contentFlag>1</contentFlag>
                    </default>
                </qualifiers>
            </content>
            <content id="ABCDOUT">
                <className>CPhsDataFile</className>
                <qualifiers>
                    <default>
                        <annotation>ModelCraft phases</annotation>
                        <subType>2</subType>
                        <contentFlag>1</contentFlag>
                    </default>
                </qualifiers>
            </content>
            <content id="PERFORMANCE">
                <className>CRefinementPerformance</className>
            </content>
        </container>
    </ccp4i2_body>
</ccp4i2>