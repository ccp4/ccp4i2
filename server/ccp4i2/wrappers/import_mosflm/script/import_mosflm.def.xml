<?xml version='1.0' encoding='ASCII'?>
<ccp4:ccp4i2 xmlns:ccp4="http://www.ccp4.ac.uk/ccp4ns">
  <ccp4i2_header>
    <function>DEF</function>
    <pluginName>import_mosflm</pluginName>
  </ccp4i2_header>
  <ccp4i2_body id="import_mosflm">
      <container id="inputData">
          <content id="MOSFLMXML">
              <className>CDataFile</className>
              <qualifiers>
                  <mustExist>True</mustExist>
                  <fileExtensions>mos,sav</fileExtensions>
                  <toolTip>XML output from iMosflm save session</toolTip>
                  <saveToDb>False</saveToDb>
              </qualifiers>
          </content>
          <content id="UNMERGEDMTZ">
              <className>CUnmergedDataFile</className>
              <qualifiers>
                  <mustExist>True</mustExist>
                  <baseName>
                      <allowedCharacters>*?</allowedCharacters>
                  </baseName>
                  <relPath>
                      <allowedCharacters>*?</allowedCharacters>
                  </relPath>
              </qualifiers>
          </content>
      </container>
      <container id="outputData">
      </container>
  </ccp4i2_body>
</ccp4:ccp4i2>
