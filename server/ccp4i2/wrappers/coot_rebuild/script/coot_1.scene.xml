<?xml version='1.0'?>
<!-- scene file for refmac model, map and difference map -->
<scene>
    <data>
    <MolData id='id1'>
       <filename database="filenames/XYZOUT"></filename>
    </MolData>
    <MapData id='id3'><filename database="filenames/FPHIIN"></filename>
     <model>id1</model>
     <contourUnits>sigma</contourUnits>
    </MapData>
    <MapData id='id4'><filename database="filenames/DELFPHIIN"></filename>
     <isDifferenceMap>True</isDifferenceMap>
     <model>id1</model>
    </MapData>
   </data>
  <View>
     <centre_MolData>id1</centre_MolData>
     <centre_selection>all</centre_selection>
     <!-- <scale_auto_contacts>1</scale_auto_contacts> -->
     <orientation_auto>
       <selection>all</selection>
       <molData>id1</molData>
     </orientation_auto>
  </View>
  <wizard><template>map and model:cylinders map symmetry sticks</template>
    <parameters>
      <MolData1>id1</MolData1>
      <MapData1>id3</MapData1> <!-- Density map -->
      <MapData2>id4</MapData2> <!-- Difference map -->
    </parameters>
  </wizard>

</scene>
