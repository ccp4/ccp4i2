<?xml version='1.0' encoding='ASCII'?>
<ccp4:ccp4i2 xmlns:ccp4="http://www.ccp4.ac.uk/ccp4ns">
  <ccp4i2_header>
    <function>DEF</function>
    <pluginName>import_xia2</pluginName>
  </ccp4i2_header>
  <ccp4i2_body id="import_xia2">
  <container id="inputData">
    <content id="XIA2_DIRECTORY">
      <className>CDataFile</className>
      <qualifiers>
        <mustExist>True</mustExist>
	<allowUndefined>False</allowUndefined>
	<saveToDb>False</saveToDb>
	<isDirectory>True</isDirectory>
        <label>XIA2 output directory</label>
	<fileExtensions>xia2</fileExtensions>
      </qualifiers>
    </content>
  </container>
  <container id="outputData">
    <content id="HKLOUT">
        <className>CList</className>
        <subItem>
          <className>CUnmergedDataFile</className>
   	  <qualifiers>
	    <saveToDb>True</saveToDb>
	  </qualifiers>
        </subItem>
    </content>
  </container>
  <container id="guiParameters">
    <content id="COPY_XIA2_DIRECTORY">
        <className>CBoolean</className>
	<qualifiers>
	    <default>False</default>
	  </qualifiers>
    </content>
   </container>
  </ccp4i2_body>
</ccp4:ccp4i2>
