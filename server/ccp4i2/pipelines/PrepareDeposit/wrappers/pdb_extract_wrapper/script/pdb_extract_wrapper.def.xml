<?xml version='1.0' encoding='ASCII'?>
<ccp4:ccp4i2 xmlns:ccp4="http://www.ccp4.ac.uk/ccp4ns">
    <ccp4i2_header>
        <function>DEF</function>
        <pluginName>pdb_extract_wrapper</pluginName>
    </ccp4i2_header>
    <ccp4i2_body id="pdb_extract_wrapper">
        <container id="inputData">
            <content id='XYZIN'>
                <className>CPdbDataFile</className>
                <qualifiers>
                    <fromPreviousJob>True</fromPreviousJob>
                    <mustExist>True</mustExist>
                    <allowUndefined>False</allowUndefined>
                </qualifiers>
            </content>
            <content id="ENTRYDATAIN">
                <className>CDataFile</className>
                <qualifiers>
                    <saveToDb>False</saveToDb>
                </qualifiers>
            </content>
            <content id="SEQUENCE_LIST">
                <className>CList</className>
                <subItem>
                    <className>CSeqDataFile</className>
                    <qualifiers>
                        <minLength>0</minLength>
                        <maxLength>0</maxLength>
                        <mustExist>True</mustExist>
                        <allowUndefined>True</allowUndefined>
                        <fromPreviousJob>True</fromPreviousJob>
                    </qualifiers>
                </subItem>
            </content>
            <content id="CHAINID_LIST">
                <className>CList</className>
                <subItem>
                    <className>CString</className>
                    <qualifiers>
                        <minLength>0</minLength>
                        <maxLength>0</maxLength>
                    </qualifiers>
                </subItem>
            </content>
        </container>
        <container id="outputData">
            <content id="ENTRYDATA">
                <className>CDataFile</className>
                <qualifiers>
                    <saveToDb>False</saveToDb>
                </qualifiers>
            </content>
            <content id="CIFFILE">
                <className>CMmcifDataFile</className>
                <qualifiers>
                    <saveToDb>False</saveToDb>
                </qualifiers>
            </content>
        </container>
    </ccp4i2_body>
</ccp4:ccp4i2>
