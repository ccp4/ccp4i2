<?xml version='1.0' encoding='ASCII'?>
<ccp4:ccp4i2 xmlns:ccp4="http://www.ccp4.ac.uk/ccp4ns">
    <ccp4i2_header>
        <function>DEF</function>
        <pluginName>hklin2cif</pluginName>
    </ccp4i2_header>
    <ccp4i2_body id="hklin2cif">
        <container id="inputData">
            <content id="HKLIN">
                <className>CMtzDataFile</className>
                <qualifiers>
                    <mustExist>True</mustExist>
                    <allowUndefined>False</allowUndefined>
                    <fromPreviousJob>True</fromPreviousJob>
                    <saveToDb>False</saveToDb>
                </qualifiers>
            </content>
            <content id="SCALEDUNMERGED">
                <className>CUnmergedDataFile</className>
                <qualifiers>
                    <allowUndefined>True</allowUndefined>
                    <mustExist>True</mustExist>
                    <guiLabel>Scaled unmerged datafile</guiLabel>
                </qualifiers>
            </content>
        </container>
        <container id="outputData">
            <content id="CIFFILE">
                <className>CMmcifDataFile</className>
                <qualifiers>
                    <saveToDb>False</saveToDb>
                </qualifiers>
            </content>
        </container>
    </ccp4i2_body>
</ccp4:ccp4i2>
