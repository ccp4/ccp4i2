{
  "name": "compounds-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "copy-rdkit": "copyfiles -u 4 node_modules/@rdkit/rdkit/dist/* public/",
    "copy-jsme": "copyfiles -u 1 assets/JSME_2020-06-11/**/* public/",
    "copy-assets": "npm run copy-rdkit && npm run copy-jsme",
    "dev": "npm run copy-assets && next dev -p 3001",
    "build": "npm run copy-assets && next build",
    "start": "next start -p 3001",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/material-nextjs": "^6.1.6",
    "@rdkit/rdkit": "^2024.3.5",
    "@tanstack/react-virtual": "^3.13.17",
    "chart.js": "^4.5.1",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.0.0",
    "seqviz": "^3.10.11",
    "swr": "^2.2.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "copyfiles": "^2.4.1",
    "typescript": "^5"
  }
}
