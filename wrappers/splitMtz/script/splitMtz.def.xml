<?xml version='1.0' encoding='ASCII'?>
<ccp4:ccp4i2 xmlns:ccp4="http://www.ccp4.ac.uk/ccp4ns">
  <ccp4i2_header>
    <function>DEF</function>
    <pluginName>splitMtz</pluginName>
  </ccp4i2_header>
  <ccp4i2_body id="splitMtz">
    <container id="inputData">
      <content id="HKLIN">
        <className>CMtzDataFile</className>
        <qualifiers>
	  <jobCombo>False</jobCombo>
	  <allowUndefined>False</allowUndefined>
	  <mustExist>True</mustExist>
	</qualifiers>
      </content>
    <content id="COLUMNGROUPLIST">  
    <className>CColumnGroupList</className>
    </content>
    <content id="USERCOLUMNGROUP">  
    <className>CColumnGroup</className>
    </content>
     </container>
    <container id="outputData">
     <content id="MINIMTZOUTLIST">
       <className>CMiniMtzDataFileList</className>
     </content>
    </container>
  </ccp4i2_body>
</ccp4:ccp4i2>
