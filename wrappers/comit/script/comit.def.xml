        <pluginVersion></pluginVersion>
        <ccp4iVersion>0.0.1</ccp4iVersion>
        <pluginName>comit</pluginName>
        <pluginTitle>Comit</pluginTitle>
        <jobId/>
    </ccp4i2_header>
    <ccp4i2_body id="comit">

        <container id="inputData">
            <content id="F_SIGF">
                <className>CObsDataFile</className>
                <qualifiers>
                    <fromPreviousJob>True</fromPreviousJob>
                    <allowUndefined>False</allowUndefined>
                    <mustExist>True</mustExist>
                </qualifiers>
            </content>
            <content id="F_PHI_IN">
                <className>CMapCoeffsDataFile</className>
                <qualifiers>
                    <mustExist>True</mustExist>
                    <fromPreviousJob>True</fromPreviousJob>
                    <sameCrystalAs>F_SIGF</sameCrystalAs>
                    <requiredSubType>1</requiredSubType>
                    <allowUndefined>True</allowUndefined>
                </qualifiers>
            </content>
        </container>

        <container id="controlParameters">
            <content id="NOMIT">
                <className>CInt</className>
                <qualifiers>
                    <default>20</default>
                    <min>8</min>
                </qualifiers>
            </content>
            <content id="PAD_RADIUS">
                <className>CFloat</className>
                <qualifiers>
                    <default>3.0</default>
                    <min>0.0</min>
                    <max>100.0</max>
                </qualifiers>
            </content>
        </container>

        <container id="outputData">
            <content id="F_PHI_OUT">
                <className>CMapCoeffsDataFile</className>
                <qualifiers>
                    <default><subType>1</subType><contentFlag>1</contentFlag></default>
                    <saveToDb>True</saveToDb>
                </qualifiers>
            </content>
        </container>

    </ccp4i2_body>
