<?xml version='1.0'?>
<ccp4i2>
    <ccp4i2_header>
        <function>DEF</function>
        <pluginName>cpatterson</pluginName>
        <userId>stuart</userId>
        <creationTime>12:40 10/Dec/14</creationTime>
        <ccp4iVersion>0.1</ccp4iVersion>
        <pluginName>cpatterson</pluginName>
        <pluginTitle>Calculate Patterson map</pluginTitle>
        <pluginVersion></pluginVersion>
    </ccp4i2_header>

    <ccp4i2_body>
        <container id="inputData">
            <content id="F_SIGF">
                <className>CObsDataFile</className>
                <qualifiers>
                    <mustExist>True</mustExist>
                    <requiredContentFlag>1,2,3,4</requiredContentFlag>
                    <allowUndefined>False</allowUndefined>
                    <fromPreviousJob>True</fromPreviousJob>
                </qualifiers>
            </content>
        </container>

        <container id="outputData">
            <content id="MAPOUT">
                <className>CMapDataFile</className>
                <qualifiers>
                    <default>
                        <subType>2</subType>
                        <contentFlag>1</contentFlag>
                    </default>
                    <allowUndefined>True</allowUndefined>
                </qualifiers>
            </content>
        </container>
    </ccp4i2_body>
</ccp4i2>
