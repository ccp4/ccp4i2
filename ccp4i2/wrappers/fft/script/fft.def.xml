<?xml version='1.0'?>
<ccp4i2>
    <ccp4i2_header>
        <function>DEF</function>
        <pluginName>fft</pluginName>
        <userId>lizp</userId>
        <creationTime>18:44 20/Dec/10</creationTime>
        <ccp4iVersion>0.1</ccp4iVersion>
        <pluginName>fft</pluginName>
        <pluginTitle>Export maps</pluginTitle>
        <pluginVersion></pluginVersion>
    </ccp4i2_header>

    <ccp4i2_body>
  
        <container id="inputData">
            <content id="FPHIIN">
                <className>CMapCoeffsDataFile</className>
                <qualifiers>
                    <mustExist>True</mustExist>
                </qualifiers>
            </content>
        
            <content id="FPHIIN2">
                <className>CMapCoeffsDataFile</className>
                <qualifiers>
                    <mustExist>False</mustExist>
                </qualifiers>
            </content>

            <content id="RESOLUTION">
                <className>CFloat</className>
            </content>

            <content id="INDEX_U">
                <className>CInt</className>
            </content>

            <content id="INDEX_V">
                <className>CInt</className>
            </content>

            <content id="INDEX_W">
                <className>CInt</className>
            </content>
        </container>


        <container id="controlParameters">
            <content id="U_VALUE">
                <className>CFloat</className>
            </content>

            <content id="B_VALUE">
                <className>CFloat</className>
            </content>
        </container>


        <container id="outputData">        
            <content id="MAPOUT">
                <className>CMapDataFile</className>
            </content>
        
            <content id="XMLOUT">
                <className>CXmlDataFile</className>
                <qualifiers>
                    <saveToDb>False</saveToDb>
                </qualifiers>
            </content>
        </container>

    </ccp4i2_body>
</ccp4i2>
