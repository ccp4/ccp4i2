<?xml version='1.0' encoding='ASCII'?>
<ccp4i2>
  <ccp4i2_header>
    <function>DEF</function>
    <userId>mdw</userId>
    <creationTime>19:15 30/Aug/11</creationTime>
    <pluginVersion></pluginVersion>
    <ccp4iVersion>0.0.1</ccp4iVersion>
    <pluginName>convert2mtz</pluginName>
    <pluginTitle></pluginTitle>
    <jobId/>
  </ccp4i2_header>
  <ccp4i2_body id="convert2mtz">
    <container id="inputData">
      <content id="HKLIN">
        <className>CGenericReflDataFile</className>
        <qualifiers>
	  <allowUndefined>False</allowUndefined>
          <mustExist>True</mustExist>
        </qualifiers>
      </content>
      <content id="SPACEGROUPCELL">
        <className>CSpaceGroupCell</className>
      </content>
      <content id="WAVELENGTH">
        <className>CWavelength</className>
	<qualifiers>
          <allowUndefined>True</allowUndefined>
        </qualifiers>
      </content>
      <content id="HKLIN_OBS_CONTENT_FLAG">
        <className>CInt</className>
      </content>
      <content id="HKLIN_OBS_COLUMNS">
        <className>CString</className>
      </content>
       <content id="HKLIN_FREER_COLUMN">
        <className>CString</className>
      </content>
      <content id="XYZIN">
        <className>CPdbDataFile</className>
	 <qualifiers>
          <mustExist>True</mustExist>
        </qualifiers>
      </content>
    </container>
    <container id="outputData">
      <content id="HKLOUT">
        <className>CMtzDataFile</className>
	<qualifiers>
          <saveToDb>True</saveToDb>
        </qualifiers>
      </content>
    </container>
    <container id="controlParameters">
      <content id="NO_COMPLETE">
        <className>CBoolean</className>
      </content>
      <content id="ANOMALOUS">
        <className>CBoolean</className>
      </content>
      <content id="SEED">
        <className>CFloat</className>
      </content>
    </container>
  </ccp4i2_body>
</ccp4i2>
