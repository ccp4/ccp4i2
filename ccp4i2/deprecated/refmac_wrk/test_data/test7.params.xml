<?xml version='1.0' encoding='ASCII'?>
<ccp4:ccp4i2 xmlns:ccp4="http://www.ccp4.ac.uk/ccp4ns">
  <ccp4i2_header>
    <function>PARAMS</function>
    <pluginName>refmac_wrk</pluginName>
  </ccp4i2_header>
  <ccp4i2_body>
    <inputData>
      <XYZIN>
        <project>FULLPATH</project>
        <relPath>/Applications/ccp4-6.2.0/examples/data</relPath>
        <baseName>gere_heavy.pdb</baseName>
      </XYZIN>
      <HKLIN>
        <project>FULLPATH</project>
        <relPath>/Applications/ccp4-6.2.0/examples/data</relPath>
        <baseName>gere.mtz</baseName>
      </HKLIN>
      <F_ANO>
        <F_plus>F(+)</F_plus>
        <SIGF_plus>SIGF(+)</SIGF_plus>
        <F_minus>F(-)</F_minus>
        <SIGF_minus>SIGF(-)</SIGF_minus>
      </F_ANO>
      <FREERFLAG>
        <FREE>FreeR_flag</FREE>
      </FREERFLAG>
    </inputData>
    <controlParameters>
      <NCYCLES>10</NCYCLES>
      <ANOMALOUS_ATOMS>
        <item>
          <atomType>SE</atomType>
          <Fp>-8.0</Fp>
          <Fpp>4.0</Fpp>
        </item>
      </ANOMALOUS_ATOMS>
      <IF_SOLVENT>False</IF_SOLVENT>
      <INPUT_PHASE>SAD</INPUT_PHASE>
      <MONI_VANDERWAALS>10.0</MONI_VANDERWAALS>
    </controlParameters>
  </ccp4i2_body>
</ccp4:ccp4i2>
