<?xml version='1.0' encoding='ASCII'?>
<ccp4:ccp4i2 xmlns:ccp4="http://www.ccp4.ac.uk/ccp4ns">
  <ccp4i2_header>
    <function>PARAMS</function>
    <pluginName>refmac_wrk</pluginName>
  </ccp4i2_header>
  <ccp4i2_body>
    <inputData>
      <XYZIN>
        <project>FULLPATH</project>
        <relPath>/Applications/ccp4-6.2.0/examples/rnase</relPath>
        <baseName>rnase.pdb</baseName>
      </XYZIN>
      <HKLIN>
        <project>FULLPATH</project>
        <relPath>/Applications/ccp4-6.2.0/examples/rnase</relPath>
        <baseName>rnase18.mtz</baseName>
      </HKLIN>
      <TLSIN>
        <project>FULLPATH</project>
        <relPath>/Applications/ccp4-6.2.0/examples/rnase</relPath>
        <baseName>rnase.tls</baseName>
      </TLSIN>
      <FOBS>
        <FP>FNAT</FP>
        <Sigma>SIGFNAT</Sigma>
      </FOBS>
      <FREERFLAG>
        <FREE>FreeR_flag</FREE>
      </FREERFLAG>
    </inputData>
    <controlParameters>
      <NCYCLES>10</NCYCLES>
      <REFINE_TYPE>TLS</REFINE_TYPE>
      <TLS_NCYCLES>20</TLS_NCYCLES>
      <BULK_SOLVENT_SCALING>BULK</BULK_SOLVENT_SCALING>
      <SCALING_IF_FIXB>True</SCALING_IF_FIXB>
      <SCALING_FIXB_BBULK>200</SCALING_FIXB_BBULK>
      <MAKE_HYDROGEN>NO</MAKE_HYDROGEN>
      <IFBFAC_SET>True</IFBFAC_SET>
      <MONI_VANDERWAALS>10.0</MONI_VANDERWAALS>
    </controlParameters>
  </ccp4i2_body>
</ccp4:ccp4i2>
